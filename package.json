{
  "type": "commonjs",
  "config": {
    "ewid": "7678",
    "version": "1.2"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --ext .ts",
    "bundle": "npm run build && npx rollup --c && cp dist/main.js bundle/main.js && cd bundle && tar -czvf edgeworker.tgz main.js bundle.json",
    "deploy:sandbox:new": "akamai sandbox add-edgeworker $npm_package_config_ewid ./bundle/edgeworker.tgz",
    "deploy:sandbox:update": "akamai sandbox update-edgeworker $npm_package_config_ewid ./bundle/edgeworker.tgz",
    "deploy:update": "akamai edgeworkers upload $npm_package_config_ewid --bundle ./bundle/edgeworker.tgz",
    "activate:latest:staging": "akamai edgeworkers activate $npm_package_config_ewid Staging $npm_package_config_version",
    "activate:latest:production": "akamai edgeworkers activate $npm_package_config_ewid Production $npm_package_config_version",
    "sandbox:start": "akamai sandbox start",
    "test:unit": "ts-node node_modules/tape/bin/tape tests/**/*.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/akamai-edgeworkers": "^1.0.8",
    "@types/sinon": "^10.0.4",
    "@types/tape": "^4.13.2",
    "@types/tape-async": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "rollup": "^2.58.0",
    "sinon": "^11.1.2",
    "tape": "^5.3.1",
    "tape-async": "^2.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.56.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
