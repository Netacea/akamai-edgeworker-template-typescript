    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "aws4": "^1.12.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "eslint": "^8.57.0",
    "querystring": "^0.2.1",
    "rollup": "^4.12.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "url": "^0.11.3"
