{
  "name": "netacea-akamai",
  "version": "1.1.0",
  "description": "Usage example of a Netacea worker on Akamai CDN",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npx tsc --project tsconfig.build.json",
