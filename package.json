{
  "name": "akamai",
  "version": "1.1.0",
  "description": "Akamai Integration Usage Example",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bundle": "npm run build && npm run rollup",
    "build": "npx tsc --project tsconfig.build.json",
    "rollup": "npx rollup -c rollup.config.js",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "test:unit": "ts-node node_modules/tape/bin/tape tests/**/*.ts",
    "ewbundle": "bash ./scripts/deploy.bash"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/akamai-edgeworkers": "^1.1.16",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.11.20",
    "@types/node-fetch": "^2.6.11",
    "@types/sinon": "^17.0.3",
    "@types/tape": "^5.6.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autocannon": "^7.15.0",
    "aws4": "^1.12.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "rollup": "^4.12.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "url": "^0.11.3"
  },
  "dependencies": {
    "@netacea/akamai": "^6.1.9"
  }
}
